                                                       Stroke="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}"/>
										</mono_system_windows_controls:Canvas>

									</mono_system_windows_controls:Border>
									<mono_system_windows_controls_primitives:Track Grid.Column="1"
                                           Name="PART_Track">
										<mono_system_windows_controls_primitives:Track.DecreaseRepeatButton>
											<mono_system_windows_controls_primitives:RepeatButton Style="{StaticResource SliderRepeatButtonStyle}"
                                                          Command="{x:Static mono_system_windows_controls:Slider.DecreaseLarge}"/>
										</mono_system_windows_controls_primitives:Track.DecreaseRepeatButton>
