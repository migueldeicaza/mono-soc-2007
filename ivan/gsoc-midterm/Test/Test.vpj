				<F N="System.Design/System.Windows.Forms.Design/ParentControlDesignerTest.cs"/>
			</Folder>
		</Folder>
		<F N="AssemblyInfo.cs"/>
		<F N="ChangeLog"/>
		<F N="Makefile"/>
	</Files>
</Project>
