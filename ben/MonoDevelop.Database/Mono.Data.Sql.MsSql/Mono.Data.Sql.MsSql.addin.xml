<Addin	id		= "Mono.Data.Sql.MsSql"
	namespace	= "Mono"
	name		= "Ms Sql provider for Mono Data Sql"
	author		= "Christian Hergert, Daniel Morgan, Sureshkumar T, Ben Motmans"
	copyright	= "MIT X11"
	url		= "http://www.monodevelop.com"
	description	= "Database Module"
	version		= "0.14.0">
    
	<Runtime>
		<Import assembly="Mono.Data.Sql.MsSql.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Mono.Data.Sql" version="0.14.0"/>
	</Dependencies>

	<ExtensionPoint path = "/Mono/Data/Sql">
		<DbFactory id = "MsSql" class = "Mono.Data.Sql.MsSqlDbFactory"/>
	</ExtensionPoint>
</Addin>