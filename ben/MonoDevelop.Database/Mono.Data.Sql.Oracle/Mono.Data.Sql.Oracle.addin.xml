<Addin	id		= "Mono.Data.Sql.Oracle"
	namespace	= "Mono"
	name		= "Oracle provider for Mono Data Sql"
	author		= "Christian Hergert, Daniel Morgan, Ben Motmans"
	copyright	= "MIT X11"
	url		= "http://www.monodevelop.com"
	description	= "Database Module"
	version		= "0.14.0">
    
	<Runtime>
		<Import assembly="Mono.Data.Sql.Oracle.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Mono.Data.Sql" version="0.14.0"/>
	</Dependencies>

	<ExtensionPoint path = "/Mono/Data/Sql">
		<DbFactory id = "Oracle" class = "Mono.Data.Sql.OracleDbFactory"/>
	</ExtensionPoint>
</Addin>