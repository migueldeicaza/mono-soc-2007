<Addin	id		= "Mono.Data.Sql.Sqlite"
	namespace	= "MonoDevelop"
	name		= "Sqlite provider for Mono Data Sql"
	author		= "Christian Hergert, Ankit Jain, Ben Motmans"
	copyright	= "MIT X11"
	url		= "http://www.monodevelop.com"
	description	= "Database Module"
	version		= "0.14.0">
    
	<Runtime>
		<Import assembly="Mono.Data.Sql.Sqlite.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Mono.Data.Sql" version="0.14.0"/>
	</Dependencies>

	<Extension path = "/Mono/Data/Sql">
		<Class id = "Sqlite" class = "Mono.Data.Sql.SqliteDbFactory"/>
	</Extension>
</Addin>